"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.constants = {
    ADD_COUNT: 'ADD_COUNT',
    REDUCE_COUNT: 'REDUCE_COUNT',
};
const initState = {
    count: 0
};
function Count(state = initState, action) {
    switch (action.type) {
        case exports.constants.ADD_COUNT: {
            return Object.assign(Object.assign({}, state), { count: state.count + 1 });
        }
        case exports.constants.REDUCE_COUNT: {
            return Object.assign(Object.assign({}, state), { count: state.count - 1 });
        }
        default: {
            return Object.assign({}, initState);
        }
    }
}
exports.default = Count;
exports.getCount = (state) => state.count.count;
